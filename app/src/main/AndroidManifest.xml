<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.micuna">

    <uses-permission android:name="android.permission.INTERNET" />



    <application
        android:name=".modelo.MicunaApp"
        android:allowBackup="true"
        android:icon="@drawable/logomicuna"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logomicuna"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
        <activity android:name=".activities.conductor.ContenidoConductor"></activity>
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.conductor.RegistroConductor" />
        <activity android:name=".activities.cliente.ContenidoCliente" />
        <activity android:name=".activities.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.cliente.RegistroCliente" />
        <activity android:name=".activities.LoginActivity" />

    </application>

</manifest>